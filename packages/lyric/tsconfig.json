{
	"include": [
		"./src/types.d.ts"
	],
	"compilerOptions": {
		"target": "ESNext",
		"lib": [
			"ESNext",
			"DOM",
			"DOM.Iterable"
		],
		"module": "ESNext",
		"strict": true,
		"noEmit": true,
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"strictNullChecks": true,
		"downlevelIteration": true,
		"allowSyntheticDefaultImports": true,
		"allowJs": false,
		"stripInternal": true,
		"noImplicitOverride": true,
		"skipLibCheck": true
	}
}